u0446\u0456\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0457 \u043d\u0430\u0432\u0447\u0430\u043b\u044c\u043d\u043e\u0457 \u043e\u043f\u0435\u0440\u0430\u0446\u0456\u0457 \"\u0406\u043d\u0442\u0435\u0440\u0444\u043b\u0435\u043a\u0441\" \u0437\u0430 \u043f\u0440\u043e\u0433\u0440\u0430\u043c\u043e\u044e \u0431\u0430\u0437\u043e\u0432\u043e\u0457 \u0437\u0430\u0433\u0430\u043b\u044c\u043d\u043e\u0432\u0456\u0439\u0441\u044c\u043a\u043e\u0432\u043e\u0457 \u043f\u0456\u0434\u0433\u043e\u0442\u043e\u0432\u043a\u0438 \u043f\u0456\u0434 \u043a\u0435\u0440\u0456\u0432\u043d\u0438\u0446\u0442\u0432\u043e\u043c \u0421\u043f\u043e\u043b\u0443\u0447\u0435\u043d\u043e\u0433\u043e \u041a\u043e\u0440\u043e\u043b\u0456\u0432\u0441\u0442\u0432\u0430 \u0412\u0435\u043b\u0438\u043a\u043e\u0457 \u0411\u0440\u0438\u0442\u0430\u043d\u0456\u0457 \u0442\u0430 \u041f\u0456\u0432\u043d\u0456\u0447\u043d\u043e\u0457 \u0406\u0440\u043b\u0430\u043d\u0434\u0456\u0457.\n</p>\n<section class=\"read\">\n <b>\n  \u0427\u0438\u0442\u0430\u0439\u0442\u0435 \u0442\u0430\u043a\u043e\u0436:\n </b>\n <a href=\"https://www.ukrinform.ua/rubric-ato/3714137-genstab-pokazav-ak-avstralijski-instruktori-u-britanii-trenuut-bijciv-zsu.html\">\n  \u0413\u0435\u043d\u0448\u0442\u0430\u0431 \u043f\u043e\u043a\u0430\u0437\u0430\u0432, \u044f\u043a \u0430\u0432\u0441\u0442\u0440\u0430\u043b\u0456\u0439\u0441\u044c\u043a\u0456\n  <b>\n   \u0456\u043d\u0441\u0442\u0440\u0443\u043a\u0442\u043e\u0440\u0438\n  </b>  \u0443 \u0411\u0440\u0438\u0442\u0430\u043d\u0456\u0457 \u0442\u0440\u0435\u043d\u0443\u044e\u0442\u044c \u0431\u0456\u0439\u0446\u0456\u0432 \u0417\u0421\u0423\n </a>\n</section>\n<p align=\"left\">\n \u0417\u0430\u0437\u043d\u0430\u0447\u0430\u0454\u0442\u044c\u0441\u044f, \u0449\u043e \u0442\u0430\u043a\u0430 \u043f\u0456\u0434\u0433\u043e\u0442\u043e\u0432\u043a\u0430 \u0434\u043e\u0437\u0432\u043e\u043b\u044f\u0454 \u0432\u0434\u043e\u0441\u043a\u043e\u043d\u0430\u043b\u0438\u0442\u0438 \u043d\u0430\u0431\u0443\u0442\u0456 \u0431\u043e\u0439\u043e\u0432\u0456 \u043d\u0430\u0432\u0438\u0447\u043a\u0438 \u0434\u043b\u044f \u043f\u0456\u0434\u0432\u0438\u0449\u0435\u043d\u043d\u044f \u043e\u0441\u043e\u0431\u0438\u0441\u0442\u043e\u0457 \u0431\u0435\u0437\u043f\u0435\u043a\u0438 \u0442\u0430 \u0431\u0435\u0437\u043f\u0435\u043a\u0438 \u043f\u0456\u0434\u0440\u043e\u0437\u0434\u0456\u043b\u0443 \u043f\u0456\u0434 \u0447\u0430\u0441 \u0440\u0456\u0437\u043d\u0438\u0445 \u0441\u0446\u0435\u043d\u0430\u0440\u0456\u0457\u0432 \u0434\u0456\u0439 \u0417\u0421\u0423, \u0449\u043e\u0431 \u0432\u0456\u0439\u0441\u044c\u043a\u043e\u0432\u0456 \u043c\u043e\u0433\u043b\u0438 \u0432\u043f\u0440\u0430\u0432\u043d\u043e \u0432\u0438\u043a\u043e\u0440\u0438\u0441\u0442\u043e\u0432\u0443\u0432\u0430\u0442\u0438 \u0442\u0430\u043a\u0442\u0438\u0447\u043d\u0456 \u0437\u043d\u0430\u043d\u043d\u044f \u0434\u043b\u044f \u0437\u0430\u0445\u0438\u0441\u0442\u0443 \u0423\u043a\u0440\u0430\u0457\u043d\u0438.\n</p>\n<p align=\"left\">\n \"\u041f\u0456\u0434 \u0435\u043a\u0441\u043f\u0435\u0440\u0442\u043d\u0438\u043c \u043a\u0435\u0440\u0456\u0432\u043d\u0438\u0446\u0442\u0432\u043e\u043c \u0456\u043d\u0441\u0442\u0440\u0443\u043a\u0442\u043e\u0440\u0456\u0432 \u0417\u0431\u0440\u043e\u0439\u043d\u0438\u0445 \u0441\u0438\u043b \u041a\u043e\u0440\u043e\u043b\u0456\u0432\u0441\u0442\u0432\u0430 \u041d\u043e\u0440\u0432\u0435\u0433\u0456\u0457 \u043d\u043e\u0432\u043e\u0431\u0440\u0430\u043d\u0446\u0456 \u0417\u0431\u0440\u043e\u0439\u043d\u0438\u0445 \u0441\u0438\u043b \u0423\u043a\u0440\u0430\u0457\u043d\u0438 \u0441\u0442\u0430\u043b\u0438 \u043f\u0456\u0434\u0433\u043e\u0442\u043e\u0432\u043b\u0435\u043d\u0456\u0448\u0438\u043c\u0438 \u0434\u043e \u0431\u043e\u0439\u043e\u0432\u0438\u0445 \u0443\u043c\u043e\u0432\", \u2014 \u0437\u0430\u0437\u043d\u0430\u0447\u0438\u043b\u0438 \u0432 \u0413\u0435\u043d\u0448\u0442\u0430\u0431\u0456.\n</p>\n<section class=\"read\">\n <b>\n  \u0427\u0438\u0442\u0430\u0439\u0442\u0435 \u0442\u0430\u043a\u043e\u0436:\n </b>\n <a href=\"https://www.ukrinform.ua/rubric-ato/3715260-u-polsi-kanadski-instruktori-vcat-ukrainskih-vijskovih-minnij-bezpeci.html\">\n  \u0423 \u041f\u043e\u043b\u044c\u0449\u0456 \u043a\u0430\u043d\u0430\u0434\u0441\u044c\u043a\u0456\n  <b>\n   \u0456\u043d\u0441\u0442\u0440\u0443\u043a\u0442\u043e\u0440\u0438\n  </b>  \u0432\u0447\u0430\u0442\u044c \u0443\u043a\u0440\u0430\u0457\u043d\u0441\u044c\u043a\u0438\u0445 \u0432\u0456\u0439\u0441\u044c\u043a\u043e\u0432\u0438\u0445 \u043c\u0456\u043d\u043d\u0456\u0439 \u0431\u0435\u0437\u043f\u0435\u0446\u0456\n </a>\n</section>\n<p align=\"left\">\n \u042f\u043a \u043f\u043e\u0432\u0456\u0434\u043e\u043c\u043b\u044f\u0432 \u0423\u043a\u0440\u0456\u043d\u0444\u043e\u0440\u043c, \u043f\u043e\u043d\u0430\u0434 10 \u0442\u0438\u0441\u044f\u0447 \u0443\u043a\u0440\u0430\u0457\u043d\u0441\u044c\u043a\u0438\u0445 \u0432\u0456\u0439\u0441\u044c\u043a\u043e\u0432\u043e\u0441\u043b\u0443\u0436\u0431\u043e\u0432\u0446\u0456\u0432 \u0443\u0436\u0435\n <a href=\"https://www.ukrinform.ua/rubric-ato/3672756-u-britanii-projsli-navcanna-ponad-10-tisac-ukrainskih-vijskovih.html\">\n  \u043f\u0440\u043e\u0439\u0448\u043b\u0438 \u043d\u0430\u0432\u0447\u0430\u043d\u043d\u044f\n </a>\n \u0443 \u0421\u043f\u043e\u043b\u0443\u0447\u0435\u043d\u043e\u043c\u0443 \u041a\u043e\u0440\u043e\u043b\u0456\u0432\u0441\u0442\u0432\u0456 \u043f\u0456\u0434 \u043a\u0435\u0440\u0456\u0432\u043d\u0438\u0446\u0442\u0432\u043e\u043c \u0421\u0443\u0445\u043e\u043f\u0443\u0442\u043d\u0438\u0445 \u0432\u0456\u0439\u0441\u044c\u043a \u0412\u0435\u043b\u0438\u043a\u043e\u0457 \u0411\u0440\u0438\u0442\u0430\u043d\u0456\u0457 \u0442\u0430 \u0417\u0431\u0440\u043e\u0439\u043d\u0438\u0445 \u0441\u0438\u043b \u043c\u0456\u0436\u043d\u0430\u0440\u043e\u0434\u043d\u0438\u0445 \u043f\u0430\u0440\u0442\u043d\u0435\u0440\u0456\u0432.\n</p>\n<p>\n</p>\n<p>\n</p>\n<p>\n</p>\n<div class=\"afterNewItemMobileBanner mobileBanner\" style=\"display:none;\">\n <div data-name=\"after_news_item_mobile\">\n </div>\n</div>"}