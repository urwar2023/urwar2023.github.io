图片: ['[630_360_16866...](https://static.ukrinform.com/photos/2023_06/thumb_files/630_360_1686686059-345.jpg)']

标签: ['МАГАТЕ', 'Запорізька АЕС', 'Галущенко', 'Гроссі']

