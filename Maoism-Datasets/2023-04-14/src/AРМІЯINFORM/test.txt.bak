    headerRadioStreamOnPayse = () => {
      headerRadioButton.classList.remove('header__radio-button--active');
      headerRadioButton.title = headerRadioButton.dataset.title;
    }
    
    headerRadioStream.addEventListener('play', headerRadioStreamOnPlay)
    headerRadioStream.addEventListener('pause', headerRadioStreamOnPayse)
    
    const onHeaderPlayClick = () => {
      if (headerRadioStream.paused) headerRadioStream.play();
      else headerRadioStream.pause();
    }
    
    headerRadioButton.addEventListener('click', onHeaderPlayClick);</script>
